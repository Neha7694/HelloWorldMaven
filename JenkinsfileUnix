pipeline { 
    agent any 
    stages {
        stage('Build') { 
            steps {
                withMaven(maven : 'apache-maven-3.3.3'){
                        sh "mvn clean compile"
                }
            }
        }
stage('Deploy') {
            steps {
               withMaven(maven : 'apache-maven-3.3.3'){
                        sh "mvn package"
                }

            }
        }
    }
}
